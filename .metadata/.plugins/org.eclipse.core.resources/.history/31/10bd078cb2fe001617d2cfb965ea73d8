package com.fetch.fbapp3.model;

public class comment {

	private int commentID;
	private int statusID;
	private int userID;
	private int flag;
	
	public comment(int commentID,int statusID,int userID,int flag)
	{
		this.commentID=commentID;
		this.statusID=statusID;
		this.userID=userID;	
		this.flag=flag;
	}
}
